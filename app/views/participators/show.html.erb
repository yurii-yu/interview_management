<div class="page-header">
  <%= link_to participators_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_participator_path(@participator), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>协同面试人</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @participator.name %></dd>

  <dt>Email:</dt>
  <dd><%= @participator.email %></dd>

  <dt>Enabled:</dt>
  <dd><%= @participator.enabled.humanize %></dd>

</dl>
